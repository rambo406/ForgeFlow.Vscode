{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
        "webview-angular-v2": {
            "projectType": "application",
            "root": "",
            "sourceRoot": "src",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "polyfills": [],
                        "tsConfig": "tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [],
                        "styles": [
                            "src/styles.scss"
                        ],
                        "scripts": [],
                        "baseHref": "./",
                        "deployUrl": "./"
                    },
                    "configurations": {
                        "production": {
                            "optimization": {
                                "scripts": true,
                                "styles": {
                                    "minify": true,
                                    "inlineCritical": false
                                },
                                "fonts": {
                                    "inline": true
                                }
                            },
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "outputHashing": "none",
                            "baseHref": "./",
                            "deployUrl": "./"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true,
                            "outputHashing": "none",
                            "baseHref": "./",
                            "deployUrl": "./"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "buildTarget": "webview-angular-v2:build:production"
                        },
                        "development": {
                            "buildTarget": "webview-angular-v2:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                }
            }
        }
    },
    "defaultProject": "webview-angular-v2"
}
